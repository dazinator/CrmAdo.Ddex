  <Views>
    <View name="Dynamics Crm">
      <DisplayName>CrmAdo Dynamics Crm</DisplayName>
      <ConnectionNode>
        <InitialDisplayName resource="Node_InitialDisplayName"></InitialDisplayName>
        <Commands>
          <!--
                        This command is a built-in command that can launch the Visual
                        Studio Query Designer to allow building a new query.  The command
                        provider is specified using a COM class ID registered in the
                        Visual Studio local registry hive.
                    -->
          <Command guid="501822E1-B5AF-11d0-B4DC-00A0C91506EF" cmdid="13608" provider="884DD964-5327-461f-9F06-6484DD540F8F" />
        </Commands>
        <Children>
          <StaticNode>
            <DisplayName resource="Node_Entities" />
            <Commands>
              <Command guid="501822E1-B5AF-11d0-B4DC-00A0C91506EF" cmdid="13608" provider="884DD964-5327-461f-9F06-6484DD540F8F" />
            </Commands>
            <Children>
              <Selection type="Tables" restrictions="{Root.Name}">
                <SelectionNode nodeId="Table">
                  <DisplayName resource="Node_Entity" />
                  <Icon name="Table"  />
                  <PropertiesRef name="EntityProperties"/>
                  <Commands>
                    <Command guid="501822E1-B5AF-11d0-B4DC-00A0C91506EF" cmdid="13608" provider="884DD964-5327-461f-9F06-6484DD540F8F" />
                  </Commands>
                  <Children>
                    <StaticNode>
                      <DisplayName resource="Node_EntityAttributes" />
                      <Icon name="Folder" />
                      <Children>
                        <Selection type="Columns" restrictions="{Table.table_catalog},{Table.table_schema},{Table.table_name}">
                          <SelectionNode>
                            <DisplayName resource="Node_EntityAttribute"/>
                            <Icon name="PrimaryKey" when ="{IsPrimaryKey} = 'true'"/>
                            <Icon name="Column" when ="{IsPrimaryKey} = 'false'"/>
                            <PropertiesRef name="AttributeProperties"/>
                          </SelectionNode>
                        </Selection>
                      </Children>
                    </StaticNode>

                    <!--<Selection type="AttributeMetadata" restrictions="{Table.LogicalName}">
                      <SelectionNode>
                        <DisplayName resource="Node_Column" />
                        -->
                    <!--<DisplayName>{Name}</DisplayName>-->
                    <!--
                        <DisplayName>{Table.LogicalName}{Column.LogicalName}</DisplayName>
                        <Icon name="Column" />
                        -->
                    <!--<Properties>
                          <TypeDisplayName>{LogicalName}</TypeDisplayName>                         
                          <Property name="Name">
                            <DisplayName>{LogicalName}</DisplayName>
                            <Category>Column Info</Category>
                            <Value>{LogicalName}</Value>
                          </Property>
                        </Properties>-->
                    <!--
                      </SelectionNode>
                    </Selection>-->
                  </Children>
                </SelectionNode>
              </Selection>
            </Children>
          </StaticNode>

          <StaticNode>
            <DisplayName resource="Node_PluginAssemblies" />
            <Children>
              <Selection type="PluginAssembly" >
                <SelectionNode nodeId="PluginAssembly">
                  <DisplayName resource="Node_PluginAssembly" />
                  <Icon name="Component"/>
                </SelectionNode>
              </Selection>
            </Children>
          </StaticNode>

          <StaticNode>
            <DisplayName resource="Node_Users" />
            <Children>
              <Selection type="User">
                <SelectionNode nodeId="User">
                  <DisplayName resource="Node_User" />
                  <Icon name="Default"/>
                </SelectionNode>
              </Selection>
            </Children>
          </StaticNode>



          <!--<StaticNode>
            <DisplayName>Tables</DisplayName>
            <Children>
              <Selection type="Table">
                <SelectionNode nodeId="Table">
